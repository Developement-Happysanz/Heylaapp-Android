<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/text_black"
    tools:context=".activity.EventImagesActivitySwipe">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/appColorBase"
        android:minHeight="60dp" />

    <ImageView
        android:id="@+id/back_res"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:minHeight="60dp"
        android:paddingEnd="15dp"
        android:paddingStart="10dp"
        android:src="@drawable/ic_arrow_back_white_24dp" />

    <TextView
        android:id="@+id/tvtitletext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/muli"
        android:gravity="center_vertical|left"
        android:minHeight="60dp"
        android:layout_toRightOf="@+id/back_res"
        android:padding="5dp"
        android:text="Gallery"
        android:textColor="@color/white"
        android:textSize="@dimen/actionbarTextSize"
        android:textStyle="bold"/>

    <com.palprotech.heylaapp.helper.AViewFlipper
        android:id="@+id/banner_new"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/tvtitletext">
    </com.palprotech.heylaapp.helper.AViewFlipper>

